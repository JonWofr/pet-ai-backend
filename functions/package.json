{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "npm run lint && tsc",
    "build:watch": "npm run lint && tsc -w",
    "serve": "firebase emulators:start --import=../firestore-emulator-data --export-on-exit",
    "serve:inspect": "firebase emulators:start --inspect-functions --import=../firestore-emulator-data --export-on-exit",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "buffer-image-size": "^0.6.4",
    "busboy": "^0.3.1",
    "express": "^4.17.1",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.6.1"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "firebase-functions-test": "^0.2.0",
    "tslint": "^5.12.0",
    "typescript": "^3.8.0"
  },
  "private": true
}
